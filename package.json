{
  "name": "coulomb",
  "version": "0.0.0-use-package-dist-json",
  "description": "Framework for building cross-platform structured data entry apps in Electron",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:riboseinc/coulomb.git",
  "author": "Ribose Inc. <open.source@ribose.com>",
  "license": "MIT",
  "scripts": {
    "prepublish": "rm -rf dist; tsc; cp package-dist.json dist/package.json; rsync -avm --include='*.js' --include='*.scss' --include='*.css' --include='*.d.ts' -f 'hide,! */' ./src/ ./dist"
  },
  "peerDependencies": {
    "@blueprintjs/core": "^3.20.0",
    "@blueprintjs/icons": "^3.12.0",
    "css-loader": "^3.0.0",
    "electron": "^8.2.4",
    "electron-log": "^4.0.6",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^3.2.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "typescript": "^3.7.2",
    "webpack": "~4.41.5"
  },
  "devDependencies": {
    "@blueprintjs/core": "^3.20.0",
    "@blueprintjs/icons": "^3.12.0",
    "@types/async-lock": "^1.1.1",
    "@types/fs-extra": "^8.0.1",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.12.27",
    "@types/node-fetch": "^2.5.4",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/throttle-debounce": "^2.1.0",
    "commitizen": "^4.1.2",
    "css-loader": "^3.2.1",
    "cz-conventional-changelog": "3.2.0",
    "electron": "^8.0.1",
    "electron-log": "^4.0.6",
    "husky": "^4.2.5",
    "node-sass": "^4.12.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "sass-loader": "^8.0.2",
    "typescript": "^3.7.2",
    "webpack": "~4.41.5"
  },
  "dependencies": {
    "async-lock": "^1.2.2",
    "fs-extra": "^8.1.0",
    "immer": "^3.1.3",
    "isomorphic-git": "^1.7.3",
    "js-yaml": "3.13.1",
    "keytar": "^6.0.1",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "throttle-debounce": "^2.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-push": "yarn"
    }
  }
}
